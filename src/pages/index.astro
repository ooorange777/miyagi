import BaseLayout from "../layouts/BaseLayout.astro";
import MainHomepage from "../components/main_homepage.astro";
import SideHomepage from "../components/side_homepage.astro";
const pageTitle = "我的主页";
const author = "ooorange";
const description = "个人博客，记录感想";
---

<BaseLayout pageTitle={pageTitle} author={author} description={description}>
